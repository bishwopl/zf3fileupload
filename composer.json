    "type": "library",
    "homepage": "https://github.com/bishwopl/file-upload",
    "keywords": [
        "form",
        "custom element"
    ],
    "authors": [
        {
            "name": "Bishwo Prasad Lamichhane",
            "email": "bishwo.prasad@gmail.com"
        }
    ],"require": {
        "php": "^5.6 || ^7",
        "zendframework/zend-cache": "^2",
        "zendframework/zend-mvc-form": "^1",
        "zendframework/zend-mvc-plugins": "^1",
        "zendframework/zend-session": "^2",
        "zendframework/zend-servicemanager-di": "^1",
        "zendframework/zend-file": "^2",
        "doctrine/doctrine-orm-module": "^1 || 2^",
        "ramsey/uuid": "^3",
        "ramsey/uuid-doctrine": "^1",
        "masterexploder/phpthumb": "^2"
    },
    "autoload": {
        "psr-4": {
